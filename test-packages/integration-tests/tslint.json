{
  "defaultSeverity": "error",
  "extends": ["../../tslint.json"],
  "jsRules": {},
  "rulesDirectory": [
    "../../node_modules/tslint-eslint-rules/dist/rules",
    "../../node_modules/tslint-plugin-blank-line/rules"
